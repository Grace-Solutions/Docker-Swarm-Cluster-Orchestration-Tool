{
  "globalSettings": {
    "clusterName": "docker-swarm-0001",
    "overlayProvider": "netbird",
    "overlayConfig": "--setup-key 'your-netbird-setup-key-here' --allow-server-ssh --enable-ssh-remote-port-forwarding --enable-ssh-sftp --enable-ssh-root",
    "setRootPassword": "",
    "servicesDir": "",
    "removeSSHPublicKeyOnCompletion": false,
    "distributedStorage": {
      "enabled": true,
      "provider": "microceph",
      "forceRecreation": false,
      "poolName": "docker-swarm",
      "providers": {
        "microceph": {
          "snapChannel": "latest/stable",
          "mountPath": "/mnt/cephfs",
          "useLoopDevices": true,
          "loopDeviceCount": 3,
          "loopDeviceSizeGB": 10,
          "enableRGW": false,
          "rgwPort": 8080
        }
      }
    },
    "preScripts": [
      {
        "enabled": false,
        "continueOnError": false,
        "name": "pre-deployment-example",
        "source": "https://example.com/scripts/pre-check.sh",
        "parameters": "--verbose",
        "conditions": []
      }
    ],
    "postScripts": [
      {
        "enabled": false,
        "continueOnError": false,
        "name": "post-deployment-example",
        "source": "https://example.com/scripts/post-validate.sh",
        "parameters": "--verbose",
        "conditions": []
      }
    ]
  },
